-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: OpenTelemetry API 
Bundle-SymbolicName: com.ibm.ws.io.opentelemetry
Bundle-Description: OpenTelemetry Java API 0.1.0, version=${bVersion} 

Export-Package: io.opentelemetry;version=0.1.0, \
  io.opentelemetry.tags;version=0.1.0, \
  io.opentelemetry.tags.spi;version=0.1.0, \
  io.opentelemetry.tags.unsafe;version=0.1.0, \
  io.opentelemetry.metrics;version=0.1.0, \
  io.opentelemetry.metrics.spi;version=0.1.0, \
  io.opentelemetry.trace;version=0.1.0, \
  io.opentelemetry.trace.spi;version=0.1.0, \
  io.opentelemetry.trace.unsafe;version=0.1.0, \
  io.opentelemetry.context;version=0.1.0, \
  io.opentelemetry.context.propagation;version=0.1.0, \
  io.opentelemetry.resource;version=0.1.0, \
  io.opentelemetry.internal;version=0.1.0, \
  io.opentelemetry.opentracingshim;version=0.1.0, \
  io.grpc;version=1.20.0

# no notice file in source jar
Include-Resource: \
  META-INF/LICENSE.txt=resources/META-INF/LICENSE.txt

instrument.disabled: true

-buildpath: \
  opentelemetry-api-0.1.0-SNAPSHOT.jar;version=file, \
  opentelemetry-opentracing-shim-0.1.0-SNAPSHOT.jar;version=file, \
  grpc-context-1.20.0.jar;version=file
